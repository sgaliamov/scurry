        </Compile>
        <Compile Update="PartialApplicationFunctions.generated.cs">
            <DesignTime>True</DesignTime>
            <AutoGen>True</AutoGen>
            <DependentUpon>PartialApplicationFunctions.tt</DependentUpon>
        </Compile>
    </ItemGroup>

    <ItemGroup>
