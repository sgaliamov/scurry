        </None>
    </ItemGroup>

    <ItemGroup>
        <Compile Update="CurryActions.generated.cs">
          <DesignTime>True</DesignTime>
          <AutoGen>True</AutoGen>
          <DependentUpon>CurryActions.tt</DependentUpon>
        </Compile>
        <Compile Update="CurryFunctions.generated.cs">
